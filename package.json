{
  "name": "remove-release-asset",
  "version": "1.0.0",
  "private": true,
  "description": "Delete a release asset from an existing GitHub Release",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint 'src/**.js' 'tests/**.js' --fix",
    "build": "ncc build src/main.ts --source-map",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flcdrg/remove-release-asset-action"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "flcdrg",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^6.16.4",
    "@types/node": "^15.12.2",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "^3.0.5",
    "prettier": "^1.16.4",
    "typescript": "^4.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/remove-release-asset.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add dist/"
    }
  }
}
