{
  "name": "remove-release-asset",
  "version": "1.0.0",
  "private": true,
  "description": "Delete a release asset from an existing GitHub Release",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint 'src/**.js' 'tests/**.js' --fix",
    "build": "ncc build src/main.ts --source-map",
    "precommit": "npm run build && git add dist/",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/remove-release-asset"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "flcdrg",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^5.0.1",
    "@types/node": "^12.12.47",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "^3.0.5",
    "prettier": "^1.16.4",
    "typescript": "^3.9.6"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/remove-release-asset.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
